name: "LeNet"
layer {
  name: "mnist"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    scale: 0.00390625
  }
  data_param {
    source: "/home/data/mnist/mnist_train_lmdb"
    batch_size: 64
    backend: LMDB
  }
}
layer {
  name: "mnist"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
    #stage: "original test"
  }
  transform_param {
    scale: 0.00390625
  }
  data_param {
    source: "/home/data/mnist/mnist_test_lmdb"
    batch_size: 100
    backend: LMDB
  }
}
